{
  "name": "music-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn run pwa && yarn run preview",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev": "vite --host",
    "pwa": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true CLAIMS=true vue-tsc --noEmit && vite build",
    "format": "prettier --write src/**/*.{ts,vue,d.ts}",
    "preview": "vite preview --host",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.4",
    "@vuelidate/core": "^2.0.0-alpha.35",
    "@vuelidate/validators": "^2.0.0-alpha.27",
    "@vueuse/core": "^8.4.2",
    "firebase": "^9.8.2",
    "howler": "^2.2.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.12",
    "vue": "^3.2.36",
    "vue-i18n": "9",
    "vue-router": "4"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@rushstack/eslint-patch": "^1.1.3",
    "@tailwindcss/forms": "^0.5.0",
    "@types/eslint": "^8.4.2",
    "@types/howler": "^2.2.7",
    "@types/node": "^17.0.36",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/cli-shared-utils": "^5.0.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.0.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.7.2",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-top-level-await": "^1.0.0",
    "vue-tsc": "^0.35.0",
    "workbox-core": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-window": "^6.5.3"
  }
}
